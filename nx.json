{
  "npmScope": "node-portfolio",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "project": {
    "express-auth0-mongoose": {
       "root": "apps/express-auth0-mongoose/",
      "sourceRoot": "apps/express-auth0-mongoose",
      "projectType": "application"
    },
    "gig-labor-api": {
      "root": "apps/gig-labor-api",
      "sourceRoot": "app/gig-labor-api/src",
      "projectType": "application"
    },
    "rxjs-rest": {
      "root": "libs/rxjs-rest",
      "sourceRoot": "libs/rxjs-rest",
      "projectType": "library"
    }
  }
  "defaultProject": "nestjs-crud"
}
